{
  "name": "portfolio",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "astro": "astro",
    "build": "npm run tsc && astro build",
    "dev": "astro dev",
    "format": "prettier --write src/**",
    "lint": "eslint --fix --no-inline-config --cache --max-warnings=0 --ext .astro,.html,.js,.jsx,.md,.mdx,.ts,.tsx src",
    "preview": "astro preview",
    "start": "astro dev",
    "tsc": "astro check && tsc --noEmit"
  },
  "dependencies": {
    "@astrojs/check": "^0.3.1",
    "@astrojs/react": "^3.0.6",
    "@astrojs/tailwind": "^5.0.2",
    "@types/react": "^18.2.40",
    "@types/react-dom": "^18.2.17",
    "astro": "^3.6.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.3.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.55.0",
    "eslint-plugin-astro": "^0.30.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "prettier": "^3.1.0",
    "prettier-plugin-astro": "^0.12.2"
  }
}
